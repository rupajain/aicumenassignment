create table employee(
   id MEDIUMINT NOT NULL AUTO_INCREMENT ,
   name VARCHAR(100) NOT NULL,
   dept VARCHAR(100),
   addr VARCHAR(300),
   skills VARCHAR(100),
   active BOOLEAN,
   PRIMARY KEY ( id )
);

